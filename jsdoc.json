{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "destination": "./docs",
    "private": true,
    "recurse": true,
    "readme": "./README.md"
  },
  "sourceType": "module",
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  }
}
